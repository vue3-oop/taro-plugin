    "dev": "unbuild",
    "build": "rimraf dist && unbuild",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@tarojs/taro": "^3.5.4",
    "injection-js": "^2.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "unbuild": "^0.8.9",
    "vue": "^3.2.37",
    "vue3-oop": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.5.4",
    "vue3-oop": "^1.0.0"
  }
}
