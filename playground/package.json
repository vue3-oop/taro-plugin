{
  "name": "playground",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:weapp": "pnpm run build:weapp -- --watch",
    "uat:weapp": "cross-env MODE=uat pnpm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "dev:h5": "pnpm run build:h5 -- --watch",
    "uat:h5": "cross-env MODE=uat pnpm run build:h5",
    "build:h5": "taro build --type h5",
    "preview:h5": "serve -s dist",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@nutui/nutui-taro": "3.1.19-beta.6",
    "@tarojs/components": "3.4.7",
    "@tarojs/taro": "3.4.7",
    "injection-js": "^2.4.0",
    "taro-axios": "^1.1.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.33",
    "vue3-oop": "^0.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@tarojs/cli": "3.4.7",
    "@tarojs/mini-runner": "3.4.7",
    "@tarojs/plugin-framework-vue3": "^3.4.7",
    "@tarojs/plugin-html": "3.4.7",
    "@tarojs/runner-utils": "^3.4.7",
    "@tarojs/runtime": "3.4.7",
    "@tarojs/service": "^3.4.7",
    "@tarojs/webpack-runner": "3.4.7",
    "@types/webpack-env": "^1.16.4",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "@vue3-oop/taro-plugin": "workspace:*",
    "autoprefixer": "^10.4.4",
    "babel-plugin-import": "^1.13.5",
    "babel-preset-taro": "3.4.7",
    "cross-env": "^7.0.3",
    "typescript": "^4.6.3",
    "vue-loader": "^17.0.0"
