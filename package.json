{
  "name": "@vue3-oop/taro-plugin",
  "version": "1.0.4",
  "description": "",
  "packageManager": "pnpm@7.0.0",
  "keywords": [],
  "homepage": "https://github.com/@vue3-oop/taro-plugin#readme",
  "bugs": {
    "url": "https://github.com/@vue3-oop/taro-plugin/issues"
  },
  "license": "MIT",
  "author": "agileago <agileago@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/@vue3-oop/taro-plugin.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@antfu/ni": "^0.17.2",
    "@babel/types": "^7.18.10",
    "@tarojs/helper": "3.5.3",
    "@tarojs/service": "^3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.8",
    "bumpp": "^8.2.1",
    "eslint": "~8.22.0",
    "esno": "^0.16.3",
    "pnpm": "^7.9.3",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.6",
    "unbuild": "^0.8.9",
    "vite": "^4.4.7",
    "vitest": "^0.22.1",
    "webpack": "5.74.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "dependencies": {
    "@babel/core": "^7",
    "@vue3-oop/jsx-hot-loader": "^0.1.6",
    "babel-loader": "^8",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "minimist": "^1.2.6",
    "ts-loader": "^9.3.1"
  }
}
